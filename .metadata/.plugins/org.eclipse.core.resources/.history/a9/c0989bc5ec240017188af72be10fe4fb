package collectionPractice;

public class Student {
	int rollno;
	String name;
	int age;
	int hashCode=0;

	public Student(int rollno, String name, int age) {
		super();
		this.rollno = rollno;
		this.name = name;
		this.age = age;
	}

	public String toString() {
		return "Student [rollno=" + rollno + ", name=" + name + ", age=" + age + "]";
	}

	public int hashCode() {
		return hashCode++;
	}
	

}
