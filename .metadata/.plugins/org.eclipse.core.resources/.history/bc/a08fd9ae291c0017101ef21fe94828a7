package com.bridgelabz.programs;

import java.util.Date;
import java.util.Scanner;

public class Stopwatch {
public static void main(String[] args) {
	
	StopwatchBussiness sw=new StopwatchBussiness();
	
		sw.start();
		
		for(int i=0;i<5000;i++)
		{
			i=i+i+i+i-i;
		}
	
	sw.stop();
    sw.timeelapsed();
}
}
