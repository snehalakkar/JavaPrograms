<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>loginRegistraionProject</display-name>
	<welcome-file-list>
		<welcome-file>emp_login.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>first</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.LoginJsp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>first</servlet-name>
		<url-pattern>/f1</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>second</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.LoginDao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>second</servlet-name>
		<url-pattern>/s1</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>third</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.RegistrationPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>third</servlet-name>
		<url-pattern>/reg</url-pattern>
	</servlet-mapping>
</web-app>